<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app id="WebApp_ID">
	<display-name>agenda-pi</display-name>
	<servlet>
		<servlet-name>SalvarServlet</servlet-name>
		<display-name>SalvarServlet</display-name>
		<description></description>
		<servlet-class>br.agenda.servlet.SalvarServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ListarServlet</servlet-name>
		<display-name>ListarServlet</display-name>
		<description></description>
		<servlet-class>br.agenda.servlet.ListarServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SalvarServlet</servlet-name>
		<url-pattern>/SalvarServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ListarServlet</servlet-name>
		<url-pattern>/ListarServlet</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
